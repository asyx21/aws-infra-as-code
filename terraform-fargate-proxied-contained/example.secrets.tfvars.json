{
  "aws_access_key": "myAWSAccessKey",
  "aws_secret_key": "myAWSSecret",
  "region": "eu-central-1",
  "app_name": "mockserver-cluster",
  "cert_arn": "arn:aws:acm:<REGION>:<ACCOUNT_ID>:certificate/<CERTIFICATE_ID>",
  "domain_name": "my.custom.domain.com",
  "private_subnets": [
    "subnet-12345678",
    "subnet-12345679"
  ],
  "echo_envs": [
    { "name": "ENABLE__ENVIRONMENT", "value": true },
    { "name": "PORT", "value": "82" }
  ],
  "proxy_envs": [
    { "name": "LOG_LEVEL", "value": "INFO" },
    { "name": "SERVER_PORT", "value": "81" },
    { "name": "PROXY_REMOTE_PORT", "value": "82" },
    { "name": "PROXY_REMOTE_HOST", "value": "127.0.0.1" }
  ]
}
